<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="bo.elite.tareasdivertidas">
    <uses-feature
        android:name="android.hardware.camera"
        android:required="true" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.EvaluacionActivity"></activity>
        <activity android:name=".Activities.EvaluacionDeMiembroActivity"></activity>
        <activity android:name=".Activities.TareasActivity"></activity>
        <activity android:name=".Activities.TareasDetailsActivity" />
        <activity android:name=".Activities.NuevaTarea" />
        <activity android:name=".Activities.AsignarMiembrosTareas"/>
        <activity android:name=".Activities.MiembroActivity" />
        <activity android:name=".Activities.FichaEditarActivity"/>
        <activity android:name=".Activities.CrearMiembroActivity" />
        <activity android:name=".Activities.MenuActivity" />
        <activity android:name=".Activities.PremiosModificarActivity"/>
        <activity android:name=".Activities.InicioActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.PremiosActivity"/>
        <activity android:name=".Activities.FichaPremioActivity"/>
        <activity android:name=".Activities.CrearPremioActivity"/>
        <activity android:name=".Activities.FichaMiembroActivity"/>
    </application>

</manifest>