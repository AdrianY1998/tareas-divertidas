<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="bo.elite.tareasdivertidas">
    <uses-feature
        android:name="android.hardware.camera"
        android:required="true" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activitys.EvaluacionActivity"></activity>
        <activity android:name=".Activitys.EvaluacionDeMiembroActivity"></activity>
        <activity android:name=".Activitys.TareasActivity"></activity>
        <activity android:name=".Activitys.TareasDetailsActivity" />
        <activity android:name=".Activitys.NuevaTarea" />
        <activity android:name=".Activitys.AsignarMiembrosTareas"/>
        <activity android:name=".Activitys.MiembroActivity" />
        <activity android:name=".Activitys.FichaEditarActivity"/>
        <activity android:name=".Activitys.CrearMiembroActivity" />
        <activity android:name=".Activitys.MenuActivity" />
        <activity android:name=".Activitys.PremiosModificarActivity"/>
        <activity android:name=".Activitys.InicioActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activitys.PremiosActivity"/>
        <activity android:name=".Activitys.FichaPremioActivity"/>
        <activity android:name=".Activitys.CrearPremioActivity"/>
        <activity android:name=".Activitys.FichaMiembroActivity"/>
    </application>

</manifest>